{ "lesson": 9, "german": "Ich bin müde von der Reise.", "czech": "Jsem unavený z cesty." },
{ "lesson": 9, "german": "Morgen fahren wir zurück.", "czech": "Zítra jedeme zpátky." },
{ "lesson": 9, "german": "Schade, dass der Urlaub vorbei ist.", "czech": "Škoda, že dovolená končí." },
{ "lesson": 9, "german": "Nächstes Jahr fahre ich nach Frankreich.", "czech": "Příští rok pojedu do Francie." },
{ "lesson": 9, "german": "Ich stelle das Auto in die Garage.", "czech": "Dávám auto do garáže." },
{ "lesson": 9, "german": "Das Auto steht in der Garage.", "czech": "Auto stojí v garáži." },
{ "lesson": 9, "german": "Leg dich ins Bett!", "czech": "Lehni si do postele!" },
{ "lesson": 9, "german": "Du liegst im Bett.", "czech": "Ležíš v posteli." },
{ "lesson": 9, "german": "Setz dich auf den Boden.", "czech": "Posaď se na zem." },
{ "lesson": 9, "german": "Er sitzt auf dem Boden.", "czech": "Sedí na zemi." },
{ "lesson": 9, "german": "Häng den Mantel an den Haken.", "czech": "Pověs kabát na háček." },
{ "lesson": 9, "german": "Der Mantel hängt am Haken.", "czech": "Kabát visí na háčku." },
{ "lesson": 9, "german": "Ich gehe auf die Post.", "czech": "Jdu na poštu." },
{ "lesson": 9, "german": "Ich bin auf der Post.", "czech": "Jsem na poště." },
{ "lesson": 10, "german": "Ich habe Fußball gespielt.", "czech": "Hrál jsem fotbal." },
{ "lesson": 10, "german": "Er hat Musik gehört.", "czech": "Poslouchal hudbu." },
{ "lesson": 10, "german": "Wir haben gelernt.", "czech": "Učili jsme se." },
{ "lesson": 10, "german": "Hast du Hausaufgaben gemacht?", "czech": "Dělal jsi domácí úkoly?" },
{ "lesson": 10, "german": "Sie hat getanzt.", "czech": "Tancovala." },
{ "lesson": 10, "german": "Ich habe ein Buch gekauft.", "czech": "Koupil jsem si knihu." },
{ "lesson": 10, "german": "Wir haben Pizza gegessen.", "czech": "Jedli jsme pizzu." },
{ "lesson": 10, "german": "Er hat Cola getrunken.", "czech": "Pil kolu." },
{ "lesson": 10, "german": "Hast du gut geschlafen?", "czech": "Spal jsi dobře?" },
{ "lesson": 10, "german": "Ich habe einen Brief geschrieben.", "czech": "Napsal jsem dopis." },
{ "lesson": 10, "german": "Sie hat ein Buch gelesen.", "czech": "Četla knihu." },
{ "lesson": 10, "german": "Wir haben ferngesehen.", "czech": "Dívali jsme se na televizi." },
{ "lesson": 10, "german": "Ich habe dich angerufen.", "czech": "Volal jsem ti." },
{ "lesson": 10, "german": "Er hat eingekauft.", "czech": "Nakoupil." },
{ "lesson": 10, "german": "Ich bin nach Hause gegangen.", "czech": "Šel jsem domů." },
{ "lesson": 10, "german": "Wir sind nach Berlin gefahren.", "czech": "Jeli jsme do Berlína." },
{ "lesson": 10, "german": "Er ist schnell gelaufen.", "czech": "Běžel rychle." },
{ "lesson": 10, "german": "Sie ist geschwommen.", "czech": "Plavala." },
{ "lesson": 10, "german": "Bist du gekommen?", "czech": "Přišel jsi?" },
{ "lesson": 10, "german": "Ich bin zu Hause geblieben.", "czech": "Zůstal jsem doma." },
{ "lesson": 10, "german": "Was ist passiert?", "czech": "Co se stalo?" },
{ "lesson": 10, "german": "Ich bin aufgestanden.", "czech": "Vstal jsem." },
{ "lesson": 10, "german": "Wann bist du eingeschlafen?", "czech": "Kdy jsi usnul?" },
{ "lesson": 10, "german": "Ich war in der Schule.", "czech": "Byl jsem ve škole." },
{ "lesson": 10, "german": "Warst du krank?", "czech": "Byl jsi nemocný?" },
{ "lesson": 10, "german": "Wir waren im Urlaub.", "czech": "Byli jsme na dovolené." },
{ "lesson": 10, "german": "Sie war müde.", "czech": "Byla unavená." },
{ "lesson": 10, "german": "Ich hatte keine Zeit.", "czech": "Neměl jsem čas." },
{ "lesson": 10, "german": "Hattest du Spaß?", "czech": "Měl jsi zábavu (bavil ses)?" },
{ "lesson": 10, "german": "Er hatte Glück.", "czech": "Měl štěstí." },
{ "lesson": 10, "german": "Wir hatten Hunger.", "czech": "Měli jsme hlad." },
{ "lesson": 10, "german": "Gutenberg hat den Buchdruck erfunden.", "czech": "Gutenberg vynalezl knihtisk." },
{ "lesson": 10, "german": "Wer hat das Telefon erfunden?", "czech": "Kdo vynalezl telefon?" },
{ "lesson": 10, "german": "Ich benutze das Internet täglich.", "czech": "Používám internet denně." },
{ "lesson": 10, "german": "Hast du ein Smartphone?", "czech": "Máš chytrý telefon?" },
{ "lesson": 10, "german": "Ich surfe im Internet.", "czech": "Surfuji na internetu." },
{ "lesson": 10, "german": "Er postet Fotos.", "czech": "Postuje fotky." },
{ "lesson": 10, "german": "Ich habe das gegoogelt.", "czech": "Vygooglil jsem to." },
{ "lesson": 10, "german": "Früher gab es keine Handys.", "czech": "Dříve nebyly mobily." },
{ "lesson": 10, "german": "Das ist eine wichtige Erfindung.", "czech": "To je důležitý vynález." },
{ "lesson": 10, "german": "Ich habe die E-Mail gelöscht.", "czech": "Smazal jsem ten e-mail." },
{ "lesson": 10, "german": "Hast du die Nachricht bekommen?", "czech": "Dostal jsi tu zprávu?" },
{ "lesson": 10, "german": "Ich habe lange gewartet.", "czech": "Čekal jsem dlouho." },
{ "lesson": 10, "german": "Er hat nicht geantwortet.", "czech": "Neodpověděl." },
{ "lesson": 10, "german": "Wir haben viel gelacht.", "czech": "Hodně jsme se smáli." },
{ "lesson": 10, "german": "Hast du das gesehen?", "czech": "Viděl jsi to?" },
{ "lesson": 10, "german": "Ich habe es vergessen.", "czech": "Zapomněl jsem to." },
{ "lesson": 10, "german": "Er hat den Schlüssel verloren.", "czech": "Ztratil klíč." },
{ "lesson": 10, "german": "Ich habe ihn getroffen.", "czech": "Potkal jsem ho." },
{ "lesson": 10, "german": "Wir haben gesprochen.", "czech": "Mluvili jsme." },
{ "lesson": 10, "german": "Sie hat gesungen.", "czech": "Zpívala." },
{ "lesson": 10, "german": "Ich habe das nicht verstanden.", "czech": "Nerozuměl jsem tomu." },
{ "lesson": 10, "german": "Er hat das Zimmer aufgeräumt.", "czech": "Uklidil pokoj." },
{ "lesson": 10, "german": "Ich habe das Fenster aufgemacht.", "czech": "Otevřel jsem okno." },
{ "lesson": 10, "german": "Hast du die Tür zugemacht?", "czech": "Zavřel jsi dveře?" },
{ "lesson": 10, "german": "Der Film hat mir gefallen.", "czech": "Film se mi líbil." },
{ "lesson": 10, "german": "Das Essen hat gut geschmeckt.", "czech": "Jídlo chutnalo dobře." },
{ "lesson": 10, "german": "Ich habe meine Freunde eingeladen.", "czech": "Pozval jsem své přátele." },
{ "lesson": 10, "german": "Wir haben gefeiert.", "czech": "Slavili jsme." },
{ "lesson": 10, "german": "Es hat geregnet.", "czech": "Pršelo." },
{ "lesson": 10, "german": "Die Sonne hat geschienen.", "czech": "Slunce svítilo." },
{ "lesson": 10, "german": "Ich habe nichts gemacht.", "czech": "Nic jsem nedělal." },
{ "lesson": 10, "german": "Das war langweilig.", "czech": "To bylo nudné." },
{ "lesson": 10, "german": "Das war super.", "czech": "To bylo super." },
{ "lesson": 10, "german": "Ich habe meine Oma besucht.", "czech": "Navštívil jsem svou babičku." },
{ "lesson": 10, "german": "Wir haben das Museum besichtigt.", "czech": "Prohlédli jsme si muzeum." },
{ "lesson": 10, "german": "Ich habe fotografiert.", "czech": "Fotografoval jsem." },
{ "lesson": 10, "german": "Hast du die Hausaufgaben schon fertig?", "czech": "Už máš hotové úkoly?" },
{ "lesson": 10, "german": "Nein, noch nicht.", "czech": "Ne, ještě ne." },
{ "lesson": 10, "german": "Ich habe es schon gemacht.", "czech": "Už jsem to udělal." },
{ "lesson": 10, "german": "Wann hast du Geburtstag gehabt?", "czech": "Kdy jsi měl narozeniny?" },
{ "lesson": 10, "german": "Letzte Woche.", "czech": "Minulý týden." },
{ "lesson": 10, "german": "Vor einem Jahr.", "czech": "Před rokem." },
{ "lesson": 10, "german": "Gestern Abend.", "czech": "Včera večer." },
{ "lesson": 10, "german": "Ich habe keine Ahnung gehabt.", "czech": "Neměl jsem tušení." },
{ "lesson": 10, "german": "Er ist mit dem Bus gefahren.", "czech": "Jel autobusem." },
{ "lesson": 10, "german": "Ich bin zu Fuß gegangen.", "czech": "Šel jsem pěšky." },
{ "lesson": 10, "german": "Wir sind geflogen.", "czech": "Letěli jsme." },
{ "lesson": 10, "german": "Ich habe mein Handy gesucht.", "czech": "Hledal jsem svůj mobil." },
{ "lesson": 10, "german": "Hast du es gefunden?", "czech": "Našel jsi ho?" },
{ "lesson": 10, "german": "Ja, es war in der Tasche.", "czech": "Ano, bylo v tašce." },
{ "lesson": 10, "german": "Ich habe Musik gehört.", "czech": "Poslouchal jsem hudbu." },
{ "lesson": 10, "german": "Er hat Gitarre gespielt.", "czech": "Hrál na kytaru." },

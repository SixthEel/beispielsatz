{
"sentences": [
{ "lesson": 5, "german": "Ich habe Hunger.", "czech": "Mám hlad." },
{ "lesson": 5, "german": "Hast du Durst?", "czech": "Máš žízeň?" },
{ "lesson": 5, "german": "Was isst du gern?", "czech": "Co jíš rád?" },
{ "lesson": 5, "german": "Ich esse gern Pizza.", "czech": "Jím rád pizzu." },
{ "lesson": 5, "german": "Isst du Fleisch?", "czech": "Jíš maso?" },
{ "lesson": 5, "german": "Nein, ich esse kein Fleisch.", "czech": "Ne, nejím maso." },
{ "lesson": 5, "german": "Das schmeckt lecker.", "czech": "To chutná lahodně." },
{ "lesson": 5, "german": "Das schmeckt nicht gut.", "czech": "To nechutná dobře." },
{ "lesson": 5, "german": "Ich trinke Wasser.", "czech": "Piji vodu." },
{ "lesson": 5, "german": "Trinkst du Kaffee?", "czech": "Piješ kávu?" },
{ "lesson": 5, "german": "Möchtest du einen Tee?", "czech": "Chtěl bys čaj?" },
{ "lesson": 5, "german": "Ich möchte eine Cola.", "czech": "Chtěl bych kolu." },
{ "lesson": 5, "german": "Wir möchten bezahlen.", "czech": "Chtěli bychom zaplatit." },
{ "lesson": 5, "german": "Die Rechnung, bitte.", "czech": "Účet, prosím." },
{ "lesson": 5, "german": "Zusammen oder getrennt?", "czech": "Dohromady nebo zvlášť?" },
{ "lesson": 5, "german": "Das macht zehn Euro.", "czech": "Dělá to deset eur." },
{ "lesson": 5, "german": "Hier ist die Speisekarte.", "czech": "Tady je jídelní lístek." },
{ "lesson": 5, "german": "Was nimmst du?", "czech": "Co si dáš?" },
{ "lesson": 5, "german": "Ich nehme das Schnitzel.", "czech": "Dám si řízek." },
{ "lesson": 5, "german": "Er nimmt einen Salat.", "czech": "On si dá salát." },
{ "lesson": 5, "german": "Sie nimmt eine Suppe.", "czech": "Ona si dá polévku." },
{ "lesson": 5, "german": "Nimmst du Pommes?", "czech": "Dáš si hranolky?" },
{ "lesson": 5, "german": "Ich mag Schokolade.", "czech": "Mám rád čokoládu." },
{ "lesson": 5, "german": "Magst du Eis?", "czech": "Máš rád zmrzlinu?" },
{ "lesson": 5, "german": "Wir mögen Obst.", "czech": "Máme rádi ovoce." },
{ "lesson": 5, "german": "Mögt ihr Gemüse?", "czech": "Máte rádi zeleninu?" },
{ "lesson": 5, "german": "Er mag keinen Käse.", "czech": "On nemá rád sýr." },
{ "lesson": 5, "german": "Zum Frühstück esse ich Brot.", "czech": "K snídani jím chléb." },
{ "lesson": 5, "german": "Zum Mittagessen esse ich Nudeln.", "czech": "K obědu jím těstoviny." },
{ "lesson": 5, "german": "Zum Abendessen esse ich nichts.", "czech": "K večeři nic nejím." },
{ "lesson": 5, "german": "Ein Brötchen mit Butter, bitte.", "czech": "Jednu housku s máslem, prosím." },
{ "lesson": 5, "german": "Der Kaffee ist heiß.", "czech": "Ta káva je horká." },
{ "lesson": 5, "german": "Das Bier ist kalt.", "czech": "To pivo je studené." },
{ "lesson": 5, "german": "Die Zitrone ist sauer.", "czech": "Citron je kyselý." },
{ "lesson": 5, "german": "Der Kuchen ist süß.", "czech": "Koláč je sladký." },
{ "lesson": 5, "german": "Die Suppe ist salzig.", "czech": "Polévka je slaná." },
{ "lesson": 5, "german": "Das Essen ist scharf.", "czech": "Jídlo je pálivé." },
{ "lesson": 5, "german": "Das schmeckt bitter.", "czech": "To chutná hořce." },
{ "lesson": 5, "german": "Das ist ungesund.", "czech": "To je nezdravé." },
{ "lesson": 5, "german": "Obst ist gesund.", "czech": "Ovoce je zdravé." },
{ "lesson": 5, "german": "Iss dein Brot!", "czech": "Sněz svůj chléb!" },
{ "lesson": 5, "german": "Trink deine Milch!", "czech": "Vypij své mléko!" },
{ "lesson": 5, "german": "Nimm das Buch!", "czech": "Vezmi si tu knihu!" },
{ "lesson": 5, "german": "Sei leise!", "czech": "Buď potichu!" },
{ "lesson": 5, "german": "Esst das Gemüse!", "czech": "Snězte tu zeleninu!" },
{ "lesson": 5, "german": "Mach die Hausaufgaben!", "czech": "Udělej domácí úkoly!" },
{ "lesson": 5, "german": "Komm her!", "czech": "Pojď sem!" },
{ "lesson": 5, "german": "Geh weg!", "czech": "Jdi pryč!" },
{ "lesson": 5, "german": "Gib mir bitte Salz.", "czech": "Podej mi prosím sůl." },
{ "lesson": 5, "german": "Hier fehlt Zucker.", "czech": "Chybí tady cukr." },
{ "lesson": 5, "german": "Ich brauche einen Löffel.", "czech": "Potřebuji lžíci." },
{ "lesson": 5, "german": "Hast du ein Messer?", "czech": "Máš nůž?" },
{ "lesson": 5, "german": "Wo ist die Gabel?", "czech": "Kde je vidlička?" },
{ "lesson": 5, "german": "Das Glas ist leer.", "czech": "Sklenice je prázdná." },
{ "lesson": 5, "german": "Der Teller ist voll.", "czech": "Talíř je plný." },
{ "lesson": 5, "german": "Ich koche gern Spaghetti.", "czech": "Rád vařím špagety." },
{ "lesson": 5, "german": "Kannst du kochen?", "czech": "Umíš vařit?" },
{ "lesson": 5, "german": "Man isst hier gut.", "czech": "Tady se jí dobře." },
{ "lesson": 5, "german": "Man trinkt viel Wasser.", "czech": "Pije se hodně vody." },
{ "lesson": 5, "german": "Man darf hier nicht essen.", "czech": "Tady se nesmí jíst." },
{ "lesson": 5, "german": "Ich möchte einen Apfel.", "czech": "Chtěl bych jablko." },
{ "lesson": 5, "german": "Hast du eine Banane?", "czech": "Máš banán?" },
{ "lesson": 5, "german": "Tomaten sind rot.", "czech": "Rajčata jsou červená." },
{ "lesson": 5, "german": "Kartoffeln sind lecker.", "czech": "Brambory jsou chutné." },
{ "lesson": 5, "german": "Ich liebe Hähnchen.", "czech": "Miluji kuře." },
{ "lesson": 5, "german": "Magst du Fisch?", "czech": "Máš rád rybu?" },
{ "lesson": 5, "german": "Nein, Fisch mag ich nicht.", "czech": "Ne, rybu nemám rád." },
{ "lesson": 5, "german": "Was ist dein Lieblingsessen?", "czech": "Jaké je tvé oblíbené jídlo?" },
{ "lesson": 5, "german": "Mein Lieblingsessen ist Pizza.", "czech": "Mé oblíbené jídlo je pizza." },
{ "lesson": 5, "german": "Trinken wir einen Saft?", "czech": "Dáme si džus?" },
{ "lesson": 5, "german": "Ja, gern.", "czech": "Ano, rád." },
{ "lesson": 5, "german": "Nein, danke.", "czech": "Ne, děkuji." },
{ "lesson": 5, "german": "Guten Appetit!", "czech": "Dobrou chuť!" },
{ "lesson": 5, "german": "Prost!", "czech": "Na zdraví! (při přípitku)" },
{ "lesson": 5, "german": "Entschuldigung, ist hier frei?", "czech": "Promiňte, je tu volno?" },
{ "lesson": 5, "german": "Ich warte auf das Essen.", "czech": "Čekám na jídlo." },
{ "lesson": 5, "german": "Das dauert lange.", "czech": "To trvá dlouho." },
{ "lesson": 5, "german": "Der Kellner kommt gleich.", "czech": "Číšník hned přijde." },
{ "lesson": 5, "german": "Möchten Sie bestellen?", "czech": "Chtěli byste si objednat?" },
{ "lesson": 5, "german": "Ich hätte gern ein Wasser.", "czech": "Dal bych si vodu." },
{ "lesson": 5, "german": "Sonst noch etwas?", "czech": "Ještě něco?" },
{ "lesson": 5, "german": "Das ist alles.", "czech": "To je vše." },
{ "lesson": 5, "german": "Schmeckt es Ihnen?", "czech": "Chutná Vám to?" },
{ "lesson": 5, "german": "Es ist ausgezeichnet.", "czech": "Je to výtečné." },
{ "lesson": 5, "german": "Das Fleisch ist zart.", "czech": "Maso je křehké." },
{ "lesson": 5, "german": "Das Brot ist frisch.", "czech": "Chléb je čerstvý." },
{ "lesson": 5, "german": "Die Milch ist sauer.", "czech": "Mléko je zkyslé." },
{ "lesson": 5, "german": "Ich kaufe Joghurt.", "czech": "Kupuji jogurt." },
{ "lesson": 5, "german": "Wir grillen im Garten.", "czech": "Grilujeme na zahradě." },
{ "lesson": 5, "german": "Isst du gern Wurst?", "czech": "Jíš rád klobásy/salám?" },
{ "lesson": 5, "german": "Ich esse lieber Käse.", "czech": "Jím raději sýr." },
{ "lesson": 5, "german": "Orangensaft schmeckt gut.", "czech": "Pomerančový džus chutná dobře." },
{ "lesson": 5, "german": "Ich trinke Tee mit Zitrone.", "czech": "Piji čaj s citronem." },
{ "lesson": 5, "german": "Kaffee mit Milch und Zucker.", "czech": "Káva s mlékem a cukrem." },
{ "lesson": 5, "german": "Ein Stück Kuchen, bitte.", "czech": "Jeden kousek koláče, prosím." },
{ "lesson": 6, "german": "Wann stehst du auf?", "czech": "Kdy vstáváš?" },
{ "lesson": 6, "german": "Ich stehe um sieben Uhr auf.", "czech": "Vstávám v sedm hodin." },
{ "lesson": 6, "german": "Er steht spät auf.", "czech": "On vstává pozdě." },
{ "lesson": 6, "german": "Wann fängt die Schule an?", "czech": "Kdy začíná škola?" },
{ "lesson": 6, "german": "Die Schule fängt um acht Uhr an.", "czech": "Škola začíná v osm hodin." },
{ "lesson": 6, "german": "Ich rufe dich an.", "czech": "Zavolám ti." },
{ "lesson": 6, "german": "Rufst du mich an?", "czech": "Zavoláš mi?" },
{ "lesson": 6, "german": "Wir kaufen heute ein.", "czech": "Dnes nakupujeme." },
{ "lesson": 6, "german": "Kauft ihr im Supermarkt ein?", "czech": "Nakupujete v supermarketu?" },
{ "lesson": 6, "german": "Er sieht gern fern.", "czech": "Rád se dívá na televizi." },
{ "lesson": 6, "german": "Ich sehe am Abend fern.", "czech": "Večer se dívám na televizi." },
{ "lesson": 6, "german": "Räum dein Zimmer auf!", "czech": "Ukliď si pokoj!" },
{ "lesson": 6, "german": "Sie räumt die Küche auf.", "czech": "Ona uklízí kuchyň." },
{ "lesson": 6, "german": "Wann kommst du zurück?", "czech": "Kdy se vrátíš?" },
{ "lesson": 6, "german": "Ich komme um sechs Uhr zurück.", "czech": "Vrátím se v šest hodin." },
{ "lesson": 6, "german": "Mach das Licht an!", "czech": "Rozsviť světlo!" },
{ "lesson": 6, "german": "Mach den Fernseher aus!", "czech": "Vypni televizi!" },
{ "lesson": 6, "german": "Ich ziehe mich an.", "czech": "Oblékám se." },
{ "lesson": 6, "german": "Er zieht eine Jacke an.", "czech": "Obléká si bundu." },
{ "lesson": 6, "german": "Ich lade dich ein.", "czech": "Zvu tě." },
{ "lesson": 6, "german": "Lädst du Freunde ein?", "czech": "Zveš kamarády?" },
{ "lesson": 6, "german": "Der Zug fährt ab.", "czech": "Vlak odjíždí." },
{ "lesson": 6, "german": "Wann fährt der Bus ab?", "czech": "Kdy odjíždí autobus?" },
{ "lesson": 6, "german": "Wie spät ist es?", "czech": "Kolik je hodin?" },
{ "lesson": 6, "german": "Es ist drei Uhr.", "czech": "Jsou tři hodiny." },
{ "lesson": 6, "german": "Es ist halb fünf.", "czech": "Je půl páté." },
{ "lesson": 6, "german": "Es ist Viertel nach zwei.", "czech": "Je čtvrt na tři." },
{ "lesson": 6, "german": "Es ist Viertel vor sechs.", "czech": "Je tři čtvrtě na šest." },
{ "lesson": 6, "german": "Es ist zehn nach zehn.", "czech": "Je deset po desáté." },
{ "lesson": 6, "german": "Es ist fünf vor zwölf.", "czech": "Je za pět minut dvanáct." },
{ "lesson": 6, "german": "Um wie viel Uhr kommst du?", "czech": "V kolik hodin přijdeš?" },
{ "lesson": 6, "german": "Ich komme um eins.", "czech": "Přijdu v jednu." },
{ "lesson": 6, "german": "Am Morgen frühstücke ich.", "czech": "Ráno snídám." },
{ "lesson": 6, "german": "Am Vormittag lerne ich.", "czech": "Dopoledne se učím." },
{ "lesson": 6, "german": "Am Mittag esse ich.", "czech": "V poledne jím." },
{ "lesson": 6, "german": "Am Nachmittag spiele ich Fußball.", "czech": "Odpoledne hraji fotbal." },
{ "lesson": 6, "german": "Am Abend lese ich.", "czech": "Večer si čtu." },
{ "lesson": 6, "german": "In der Nacht schlafe ich.", "czech": "V noci spím." },
{ "lesson": 6, "german": "Was machst du heute?", "czech": "Co děláš dnes?" },
{ "lesson": 6, "german": "Ich besuche Oma.", "czech": "Navštívím babičku." },
{ "lesson": 6, "german": "Wir besuchen ein Museum.", "czech": "Navštívíme muzeum." },
{ "lesson": 6, "german": "Ich verstehe dich nicht.", "czech": "Nerozumím ti." },
{ "lesson": 6, "german": "Hörst du mich?", "czech": "Slyšíš mě?" },
{ "lesson": 6, "german": "Ich liebe dich.", "czech": "Miluji tě." },
{ "lesson": 6, "german": "Er fragt sie.", "czech": "On se jí ptá." },
{ "lesson": 6, "german": "Sie fragt ihn.", "czech": "Ona se ho ptá." },
{ "lesson": 6, "german": "Wir sehen uns.", "czech": "Uvidíme se." },
{ "lesson": 6, "german": "Ich kaufe es für dich.", "czech": "Koupím to pro tebe." },
{ "lesson": 6, "german": "Das ist für ihn.", "czech": "To je pro něj." },
{ "lesson": 6, "german": "Das ist für sie.", "czech": "To je pro ni." },
{ "lesson": 6, "german": "Ohne dich gehe ich nicht.", "czech": "Bez tebe nejdu." },
{ "lesson": 6, "german": "Wir fahren ohne ihn.", "czech": "Jedeme bez něj." },
{ "lesson": 6, "german": "Gegen wen spielt ihr?", "czech": "Proti komu hrajete?" },
{ "lesson": 6, "german": "Wir spielen gegen sie.", "czech": "Hrajeme proti nim." },
{ "lesson": 6, "german": "Ich gehe durch den Park.", "czech": "Jdu parkem (skrz park)." },
{ "lesson": 6, "german": "Der Weg führt durch den Wald.", "czech": "Cesta vede lesem." },
{ "lesson": 6, "german": "Um wie viel Uhr beginnt der Film?", "czech": "V kolik hodin začíná film?" },
{ "lesson": 6, "german": "Der Film beginnt um 20 Uhr.", "czech": "Film začíná ve 20 hodin." },
{ "lesson": 6, "german": "Wen suchst du?", "czech": "Koho hledáš?" },
{ "lesson": 6, "german": "Ich suche meinen Bruder.", "czech": "Hledám svého bratra." },
{ "lesson": 6, "german": "Wen rufst du an?", "czech": "Komu voláš?" },
{ "lesson": 6, "german": "Ich rufe meine Mutter an.", "czech": "Volám své matce." },
{ "lesson": 6, "german": "Ich bereite das Frühstück zu.", "czech": "Připravuji snídani." },
{ "lesson": 6, "german": "Bereitest du dich vor?", "czech": "Připravuješ se?" },
{ "lesson": 6, "german": "Ich schlafe gleich ein.", "czech": "Hned usnu." },
{ "lesson": 6, "german": "Er schläft im Unterricht ein.", "czech": "On usíná při vyučování." },
{ "lesson": 6, "german": "Ich habe keine Zeit für dich.", "czech": "Nemám na tebe čas." },
{ "lesson": 6, "german": "Hast du Zeit für mich?", "czech": "Máš na mě čas?" },
{ "lesson": 6, "german": "Er duscht am Morgen.", "czech": "Sprchuje se ráno." },
{ "lesson": 6, "german": "Sie badet am Abend.", "czech": "Koupe se večer." },
{ "lesson": 6, "german": "Ich putze mir die Zähne.", "czech": "Čistím si zuby." },
{ "lesson": 6, "german": "Wann frühstückst du?", "czech": "Kdy snídáš?" },
{ "lesson": 6, "german": "Ich frühstücke um halb acht.", "czech": "Snídám v půl osmé." },
{ "lesson": 6, "german": "Gehst du heute joggen?", "czech": "Jdeš dnes běhat?" },
{ "lesson": 6, "german": "Nein, ich bin müde.", "czech": "Ne, jsem unavený." },
{ "lesson": 6, "german": "Wir gehen spazieren.", "czech": "Jdeme na procházku." },
{ "lesson": 6, "german": "Ich muss noch lernen.", "czech": "Musím se ještě učit." },
{ "lesson": 6, "german": "Hast du heute Stress?", "czech": "Máš dnes stres?" },
{ "lesson": 6, "german": "Ja, ich habe viel zu tun.", "czech": "Ano, mám hodně práce." },
{ "lesson": 6, "german": "Die Woche hat sieben Tage.", "czech": "Týden má sedm dní." },
{ "lesson": 6, "german": "Montag, Dienstag, Mittwoch.", "czech": "Pondělí, úterý, středa." },
{ "lesson": 6, "german": "Donnerstag, Freitag, Samstag.", "czech": "Čtvrtek, pátek, sobota." },
{ "lesson": 6, "german": "Sonntag ist Ruhetag.", "czech": "Neděle je den klidu." },
{ "lesson": 6, "german": "Welchen Tag haben wir heute?", "czech": "Co je dnes za den?" },
{ "lesson": 6, "german": "Heute ist Freitag.", "czech": "Dnes je pátek." },
{ "lesson": 6, "german": "Morgen ist Samstag.", "czech": "Zítra je sobota." },
{ "lesson": 6, "german": "Übermorgen ist Sonntag.", "czech": "Pozítří je neděle." },
{ "lesson": 6, "german": "Gestern war Donnerstag.", "czech": "Včera byl čtvrtek." },
{ "lesson": 6, "german": "Vorgestern war Mittwoch.", "czech": "Předevčírem byla středa." },
{ "lesson": 6, "german": "Ich mag das Wochenende.", "czech": "Mám rád víkend." },
{ "lesson": 6, "german": "Endlich Wochenende!", "czech": "Konečně víkend!" },
{ "lesson": 7, "german": "Ich helfe dir.", "czech": "Pomůžu ti." },
{ "lesson": 7, "german": "Kannst du mir helfen?", "czech": "Můžeš mi pomoci?" },
{ "lesson": 7, "german": "Das gefällt mir.", "czech": "To se mi líbí." },
{ "lesson": 7, "german": "Gefällt dir das Buch?", "czech": "Líbí se ti ta kniha?" },
{ "lesson": 7, "german": "Das T-Shirt gefällt mir nicht.", "czech": "To tričko se mi nelíbí." },
{ "lesson": 7, "german": "Wie geht es dir?", "czech": "Jak se máš?" },
{ "lesson": 7, "german": "Mir geht es gut.", "czech": "Mám se dobře." },
{ "lesson": 7, "german": "Wie geht es ihm?", "czech": "Jak se má on?" },
{ "lesson": 7, "german": "Es geht ihm schlecht.", "czech": "Má se špatně." },
{ "lesson": 7, "german": "Wie geht es ihr?", "czech": "Jak se má ona?" },
{ "lesson": 7, "german": "Es geht ihr super.", "czech": "Má se skvěle." },
{ "lesson": 7, "german": "Gehört das dir?", "czech": "Patří to tobě?" },
{ "lesson": 7, "german": "Ja, das gehört mir.", "czech": "Ano, to patří mně." },
{ "lesson": 7, "german": "Nein, das gehört ihm.", "czech": "Ne, to patří jemu." },
{ "lesson": 7, "german": "Das gehört uns.", "czech": "To patří nám." },
{ "lesson": 7, "german": "Gehört das euch?", "czech": "Patří to vám?" },
{ "lesson": 7, "german": "Das Auto gehört ihnen.", "czech": "To auto patří jim." },
{ "lesson": 7, "german": "Ich schenke dir ein Buch.", "czech": "Daruji ti knihu." },
{ "lesson": 7, "german": "Was schenkst du ihm?", "czech": "Co mu daruješ?" },
{ "lesson": 7, "german": "Ich schenke ihr Blumen.", "czech": "Daruji jí květiny." },
{ "lesson": 7, "german": "Wir schenken euch nichts.", "czech": "Nic vám nedarujeme." },
{ "lesson": 7, "german": "Sie schenken uns Schokolade.", "czech": "Darují nám čokoládu." },
{ "lesson": 7, "german": "Ich danke dir.", "czech": "Děkuji ti." },
{ "lesson": 7, "german": "Wir danken Ihnen.", "czech": "Děkujeme Vám." },
{ "lesson": 7, "german": "Das schmeckt mir.", "czech": "To mi chutná." },
{ "lesson": 7, "german": "Schmeckt es dir?", "czech": "Chutná ti to?" },
{ "lesson": 7, "german": "Das passt mir.", "czech": "To mi sedí / To se mi hodí." },
{ "lesson": 7, "german": "Die Hose passt ihm nicht.", "czech": "Ty kalhoty mu nesedí." },
{ "lesson": 7, "german": "Ich antworte dir später.", "czech": "Odpovím ti později." },
{ "lesson": 7, "german": "Antworte mir!", "czech": "Odpověz mi!" },
{ "lesson": 7, "german": "Er gibt mir die Hand.", "czech": "Podává mi ruku." },
{ "lesson": 7, "german": "Gibst du mir den Kuli?", "czech": "Podáš mi to pero?" },
{ "lesson": 7, "german": "Ich zeige dir mein Haus.", "czech": "Ukážu ti svůj dům." },
